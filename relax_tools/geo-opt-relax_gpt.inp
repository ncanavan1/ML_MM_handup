&GLOBAL
  PRINT_LEVEL low
  PROJECT dy_complex
  RUN_TYPE GEO_OPT
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ../BASIS_MOLOPT_UCL
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME ../POTENTIAL
    CHARGE +3
    UKS
    MULTIPLICITY MLTP
    &MGRID
      CUTOFF 500
      NGRIDS 4
      REL_CUTOFF 30
    &END MGRID
    &SCF
      MAX_SCF 200
      EPS_SCF 5.0E-5
      SCF_GUESS RESTART
      &OT
        MINIMIZER DIIS
        PRECONDITIONER FULL_SINGLE_INVERSE
      &END OT
    &END SCF
   &XC
   &XC_FUNCTIONAL
    &PBE
    &END PBE
   &END XC_FUNCTIONAL
  &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 25 25 25
      PERIODIC XYZ
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME ../../frame.xyz
      COORD_FILE_FORMAT XYZ
    &END TOPOLOGY
     &KIND Dy
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q20
  &END KIND
  &KIND Tb
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q19
  &END KIND
  &KIND Ho
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q21
  &END KIND
  &KIND Gd
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q18
  &END KIND
  &KIND Lu
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q25
  &END KIND
  &KIND Pm
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q15
  &END KIND
  &KIND Tm
   BASIS_SET DZVP-MOLOPT-SR-GTH
   POTENTIAL GTH-PBE-q23 
  &END KIND
  &KIND F
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q7
  &END KIND
  &KIND S
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q6
  &END KIND
  &KIND Na
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q9
  &END KIND
  &KIND B
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q3
  &END KIND
  &KIND Cl
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q7
  &END KIND
  &KIND H
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q1
  &END KIND
  &KIND N
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q5
  &END KIND
  &KIND O
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q6
  &END KIND
  &KIND C
   BASIS_SET DZVP-GTH-PADE
   POTENTIAL GTH-PBE-q4
  &END KIND
  &END SUBSYS
&END FORCE_EVAL

&MOTION
  &GEO_OPT
    TYPE MINIMIZATION
    MAX_ITER 100
    MAX_DR    1.0E-01
    MAX_FORCE 1.0E-01
    RMS_DR    1.0E-02
    RMS_FORCE 1.0E-02
    OPTIMIZER BFGS
    &BFGS
      TRUST_RADIUS 0.3
    &END BFGS
  &END GEO_OPT
&END MOTION
